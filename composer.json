{
    "name": "omasn/reflection-validator",
    "description": "Extends the capability of the symfony validator",
    "license": "MIT",
    "keywords": [
        "php",
        "symfony-validator",
        "reflection-validator"
    ],
    "homepage": "https://github.com/code-pilots/change-set-collection",
    "type": "library",
    "authors": [
        {
            "name": "Roman Yastrebov",
            "email": "tirastwo@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omasn\\ReflectionValidator\\": "src/"
        }
    },
    "require": {
        "php": ">=8.2",
        "symfony/validator": "^6.3",
        "symfony/property-info": "^6.3"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5",
        "symfony/var-dumper": "^4.0|^5.4",
        "vimeo/psalm": "^4.30"
    }
}
